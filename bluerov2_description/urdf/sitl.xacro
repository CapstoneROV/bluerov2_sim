            <i_min>0</i_min>
            <cmd_max>51</cmd_max>
            <cmd_min>-40</cmd_min>
            <linkName>bluerov2/thruster_sitl_2</linkName>
            <jointName>bluerov2/thruster_sitl_2_joint</jointName>
            <multiplier>75</multiplier>
            </control>

            <control channel="3">
